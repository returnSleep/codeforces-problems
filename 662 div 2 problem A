#include <iostream>
using namespace std;
 
int main() {
//	cout<<"GfG!";
int t,n,i,j,k,a,b,c,d;
cin>>t;
while(t--)
{
    cin>>n;
    if(n<31)
    cout<<"NO"<<endl;
    else if(n==31)
      {   cout<<"YES"<<endl;
          cout<<"6 14 10 1"<<endl;
      }else if(n==36)
      {cout<<"YES"<<endl;
         cout<<"5 15 10 6"<<endl;
          
      }else if(n==40)
      {
          cout<<"YES"<<endl;
          cout<<"6 15 14 5"<<endl;
      }else if(n==44)
      { cout<<"YES"<<endl;
          cout<<"6 15 10 13"<<endl;
          
      }else
      {
          cout<<"YES"<<endl;
          cout<<"6 10 14 "<<n-30<<endl;
      }
}
 
	return 0;
}
